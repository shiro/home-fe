{
  "name": "home-fe",
  "version": "1.0.0",
  "description": "Homepage fe",
  "scripts": {
    "setup:report": "mkdir -p report/cobertura && mkdir -p report/junit && mkdir -p report/checkstyle",
    "prod": "npm run prod:build && npm run prod:run",
    "prod:build": "concurrently \"npm run prod:server:build\" \"npm run prod:client:build\"",
    "prod:run": "npm run prod:server:run",
    "prod:server:build": "webpack --progress=false --config webpack/webpack.server.prod.config.js",
    "prod:server:run": "node ./dist_server/server",
    "prod:client:build": "webpack --progress=false --config webpack/webpack.client.prod.config.js",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "webpack --config webpack/webpack.server.config.js",
    "dev:client": "webpack-dev-server --config webpack/webpack.client.config.js",
    "test": "jest --config jest.config.json --updateSnapshot",
    "test:report": "npm run setup:report && npm run test --coverage",
    "lint:report": "npm run setup:report && npm run lint:es -f checkstyle -o report/checkstyle/eslint.xml || true",
    "lint": "eslint --ext js,jsx,ts,tsx src",
    "lint:fix": "npm run lint --fix || true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shiro/home-fe"
  },
  "keywords": [
    "javascript",
    "react",
    "es6",
