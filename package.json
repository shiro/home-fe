{
  "name": "home-fe",
  "version": "0.0.1-SNAPSHOT",
  "description": "",
  "scripts": {
    "prod": "yarn prod:build && yarn prod:run",
    "prod:build": "concurrently \"yarn prod:server:build\" \"yarn prod:client:build\"",
    "prod:run": "yarn prod:server:run",
    "prod:server:build": "webpack --progress=false --config webpack/webpack.server.prod.config.js",
    "prod:server:run": "cross-env NODE_ENV=production node ./dist_server/server",
    "prod:client:build": "webpack --progress=false --config webpack/webpack.client.prod.config.js",
    "dev": "cross-env concurrently \"yarn dev:server\" \"yarn dev:client\"",
    "dev:server": "webpack --config webpack/webpack.server.config.js",
    "dev:client": "webpack-dev-server --config webpack/webpack.client.config.js",
    "test": "jest --config jest.config.json --updateSnapshot",
    "test:report": "yarn report:setup && yarn test --coverage",
    "lint": "yarn lint:es",
    "lint:report": "yarn report:setup && yarn lint:es:report",
    "lint:fix": "yarn lint:es:fix",
    "lint:es": "eslint --ext js,jsx,ts,tsx src",
    "lint:es:report": "yarn lint:es -f checkstyle -o report/checkstyle/eslint.xml || true",
    "lint:es:fix": "yarn lint:es --fix || true",
    "report:setup": "mkdir -p report/cobertura && mkdir -p report/junit && mkdir -p report/checkstyle"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shiro/home-fe"
  },
  "keywords": [
    "javascript",
    "react",
    "es6",
    "express",
    "webpack",
    "babel",
    "docker",
    "maven"
  ],
  "license": "MIT",
  "homepage": "usagi.io",
