{
  "name": "home-fe",
  "version": "0.0.1-SNAPSHOT",
  "description": "",
  "scripts": {
    "prod": "yarn prod:build && yarn prod:run",
    "prod:build": "concurrently \"yarn prod:server:build\" \"yarn prod:client:build\"",
    "prod:run": "yarn prod:server:run",
    "prod:server:build": "webpack --progress=false --config webpack/server.prod.config.js",
    "prod:server:run": "cross-env NODE_ENV=production node ./dist_server/server",
    "prod:client:build": "webpack --progress=false --config webpack/client.prod.config.js",
    "dev": "cross-env concurrently \"yarn dev:server\" \"yarn dev:client\"",
    "dev:server": "webpack --config webpack/server.config.js",
    "dev:client": "webpack-dev-server --config webpack/client.config.js",
    "test": "jest --config jest.config.json --updateSnapshot",
    "lint": "concurrently \"yarn lint:es\" \"yarn lint:ts\"",
    "lint:fix": "concurrently \"yarn lint:es:fix\" \"yarn lint:ts:fix\"",
    "lint:es": "eslint --ext js,jsx,ts,tsx src",
    "lint:ts": "tslint -p .",
    "lint:es:fix": "yarn lint:es --fix",
    "lint:ts:fix": "yarn lint:ts --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shiro/home-fe"
  },
  "keywords": [
    "javascript",
    "react",
    "es6",
    "express",
    "webpack",
    "babel",
    "docker",
    "maven"
  ],
  "license": "MIT",
  "homepage": "usagi.io",
